{
  "name": "silverback-cli",
  "description": "The Silverback Command Line",
  "version": "0.0.0",
  "author": "Anthony Madhvani",
  "bin": {
    "silverback": "./bin/run"
  },
  "bugs": "https://github.com/icapps/nodejs-silverback-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^1",
    "lodash": "^4.17.10",
    "nodegit": "^0.22.0",
    "nunjucks": "^3.1.2",
    "pluralize": "^7.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^11",
    "ts-node": "^6",
    "tslint": "^5",
    "typescript": "^2.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/icapps/nodejs-silverback-cli",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "silverback",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "icapps/nodejs-silverback-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
